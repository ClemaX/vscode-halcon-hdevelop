{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Halcon HDevelop API",
	"patterns": [
		{
			"include": "#storage"
		}
	],
	"repository": {
		"storage": {
			"patterns": [
				{
					"name": "storage.modifier.hdevelop",
					"match": "\\b(iconic|ctrl)\\b"
				}
			]
		}
	},
	"scopeName": "source.hdevelop.api"
}